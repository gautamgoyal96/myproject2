    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script type="text/javascript">
        
        var pathname = window.location.pathname;
        var res = pathname.replace("/", "");
        var res = res.split("/");
        var res = res[0];
          $('.common').removeClass("active");
        if(res){

           switch(res) {
                case 'artist':
                    $('#user').addClass("active");
                    break;
                case 'artistview':
                    $('#user').addClass("active");
                    res = 'artist';
                    break;
                case 'customer':
                    $('#user').addClass("active");
                    break;
                case 'customerview':
                    $('#user').addClass("active");
                     res = 'customer';
                    break;    
                case 'addSubCategory':
                    $('#sub_service').addClass("active");
                    break;
                case 'listSubCategory':
                    $('#sub_service').addClass("active");
                    break; 
                case 'categorySubEdit':
                    $('#sub_service').addClass("active");
                    break;        
                case 'addCategory':
                    $('#service').addClass("active");
                    break;   
                case 'listCategory':
                    $('#service').addClass("active");
                    break;
                case 'categoryEdit':
                    $('#service').addClass("active");
                    break;
                case 'admin_payment_history':
                    $('#payment').addClass("active");
                case 'bookingview':
                    $('#payment').addClass("active");
                    res = 'admin_payment_history';    
                    break;
                case 'total_services':
                    $('#Tservices').addClass("active");
                    break;                           
                default:
                    $('#adminDashboard').addClass("active");
            } 

          $('#'+res).addClass("active");
        
        }

    </script>
