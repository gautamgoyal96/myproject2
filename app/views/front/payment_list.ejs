                  <th>Category</th>
                  <th>Date</th>
                  <th>Staff Name</th>
                  <th>Price</th>
                  <th>Status</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                  <% 
 var startTime = (Number(currentPage)-1)*10;
   i=1+startTime;
                  if(listdata.length>0){
   listdata.forEach(function(rs) { %>
                <tr>
                  <td><%= i %></td>
                  <td>
                    <div class="paymentuser">
                      <div class="media">
