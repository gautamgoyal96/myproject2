
<% if(slot.length){ 
i=0;
slot.forEach(function(rs) {

 %>

  <span>
    <input id="time<%= i %>" type="radio" name="timeslot" value="<%= rs %>" onclick="selectslot(this);" data-vali = "<%= i %>" class="timeslot">
    <label for="time<%= i %>"><%= rs %></label>
  </span>

   <%if(selectData[i]==1){%>
 <script type="text/javascript">

 	$("#time"+<%= i %>).click();
 
 </script>
<%}%>

<% i++;}); }else{ %>

<p  style="font-size: 20px;font-weight: 500;text-align: center;">

No slot available

</p>

<% }%>
 

